<% if flash[:error] %>
  <% flash[:error].each do |msg| %>
    <p><%= msg %></p>
  <% end %>
<% end %>

<%= form_for @student do |f| %>
<%= f.label :name %>
<%= f.text_field :name %>
<%= f.label :mod %>
<%= f.number_field :mod %>
<%= f.collection_select :duck_ids, @ducks, :id, :name %>
<%= f.submit %>
<% end %>
