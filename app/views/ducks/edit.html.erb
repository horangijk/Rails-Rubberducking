<% if flash[:error] %>
  <% flash[:error].each do |msg| %>
    <p><%= msg %></p>
  <% end %>
<% end %>

<%= form_for @duck do |f| %>
<%= f.label :name %>
<%= f.text_field :name %>
<%= f.label :description %>
<%= f.number_field :description %>
<%= f.collection_select :student_ids, @students, :id, :name %>
<%= f.submit %>
<% end %>
